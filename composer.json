{
    "name": "ffi/env",
    "description": "PHP FFI Environment",
    "license": "MIT",
    "keywords": ["ffi", "env", "environment"],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "FFI\\Env\\": "src"
        }
    },
    "require-dev": {
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "FFI\\Env\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
